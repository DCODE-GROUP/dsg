{
    "name": "dcodegroup/dsg",
    "description": "UI components for livewire 3.",
    "type": "library",
    "require": {
        "spatie/laravel-package-tools": "^1.16",
        "blade-ui-kit/blade-heroicons": "^2.0",
        "php": "^8.0|^8.1|^8.2",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.42",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^8.21",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/laravel-log-dumper": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Dcodegroup\\Dsg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dcodegroup\\Dsg\\Tests": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nathan Rzepecki",
            "email": "nathan@lionslair.net.au",
            "homepage": "https://www.lionslair.net.au"
        },
        {
            "name": "Dcode Group",
            "email": "forge@dcodegroup.com",
            "homepage": "https://dcodegroup.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
